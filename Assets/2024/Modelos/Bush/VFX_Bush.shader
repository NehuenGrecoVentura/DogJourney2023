			float2 uv_BushOpacity = i.uv_texcoord * _BushOpacity_ST.xy + _BushOpacity_ST.zw;
			o.Alpha = tex2D( _BushOpacity, uv_BushOpacity ).r;
		}

		ENDCG
		CGPROGRAM
		#pragma surface surf Standard keepalpha fullforwardshadows 

		ENDCG
		Pass
		{
			Name "ShadowCaster"
			Tags{ "LightMode" = "ShadowCaster" }
